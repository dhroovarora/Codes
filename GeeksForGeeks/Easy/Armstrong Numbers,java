https://www.geeksforgeeks.org/problems/armstrong-numbers2727/1




class Solution {
    static String armstrongNumber(int n){
        // code here
        int a = n%10;
        int b = (n/10)%10;
        int c = (n/100)%100;
        if
        (a*a*a + b*b*b + c*c*c == n)
            return "Yes";
        return "No";
    }
}